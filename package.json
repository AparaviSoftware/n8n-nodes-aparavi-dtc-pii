{
  "name": "n8n-nodes-aparavi-dtc-pii",
  "version": "1.0.1",
  "description": "n8n community node for PII anonymization using Aparavi DTC API. Detect and censor personally identifiable information in n8n workflows.",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "n8n-node",
    "n8n-community",
    "pii",
    "anonymize",
    "anonymization",
    "censor",
    "censoring",
    "privacy",
    "data-protection",
    "aparavi",
    "aparavi-dtc",
    "gdpr",
    "hipaa",
    "personally-identifiable-information",
    "data-privacy",
    "workflow",
    "automation"
  ],
  "license": "MIT",
  "homepage": "https://github.com/AparaviSoftware/n8n-nodes-aparavi-dtc-pii",
  "author": {
    "name": "James Tucker",
    "email": "james@example.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AparaviSoftware/n8n-nodes-aparavi-dtc-pii.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint . --ext .js,.ts --ignore-path .gitignore",
    "lintfix": "eslint . --ext .js,.ts --ignore-path .gitignore --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AparaviApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/AparaviPII/AparaviPII.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^16.18.68",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.42.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "aparavi-universal-pii": "file:./aparavi-universal-pii.js"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
