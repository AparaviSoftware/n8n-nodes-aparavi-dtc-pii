{
  "name": "n8n-nodes-aparavi-pii",
  "version": "1.0.0",
  "description": "n8n community node for Aparavi PII (Personally Identifiable Information) censoring",
  "keywords": [
    "n8n-community-node-package",
    "pii",
    "privacy",
    "data-protection",
    "aparavi",
    "gdpr",
    "hipaa"
  ],
  "license": "MIT",
  "homepage": "https://github.com/jamestucker/n8n-pii-node",
  "author": {
    "name": "James Tucker",
    "email": "james@example.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jamestucker/n8n-pii-node.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -s"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AparaviApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/AparaviPII/AparaviPII.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^16.18.68",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.42.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "aparavi-universal-pii": "file:./aparavi-universal-pii.js"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}

